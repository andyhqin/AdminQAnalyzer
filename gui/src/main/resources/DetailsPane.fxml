<?import javafx.scene.control.cell.PropertyValueFactory?>

<GridPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <TableView fx:id="DetailTable" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn prefWidth="150.0" text="Key">
            <cellValueFactory><PropertyValueFactory property="OpCode" /></cellValueFactory>
          </TableColumn>
          <TableColumn prefWidth="150.0" text="Value" />
        </columns>
      </TableView>
   </children>
</GridPane>
